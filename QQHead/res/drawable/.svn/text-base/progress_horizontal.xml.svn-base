<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                    android:startColor="#8e8e8e"
                    android:centerColor="#8e8e8e"
                    android:centerY="0.75"
                    android:endColor="#8e8e8e"
                    android:angle="270"/>
             <stroke  
	                 android:width="1dp"  
	                 android:color="#ffffff"  
	                 android:dashWidth="5dp"/> 
        </shape>
    </item>
    
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                        android:startColor="#80ffd300"
                        android:centerColor="#80ffb600"
                        android:centerY="0.75"
                        android:endColor="#a0ffcb00"
                        android:angle="270"/>
            </shape>
        </clip>
    </item>
    
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#9aff02"
                    android:centerColor="#73bf00"
                    android:centerY="0.5"
                    android:endColor="#467500"
                    android:angle="270" />
                 <stroke
	                 android:width="1dp"  
	                 android:color="#ffffff"  
	                 android:dashWidth="5dp"/> 
							<padding android:left="7dp" android:top="7dp" android:right="7dp"
                android:bottom="7dp" />
            </shape>
        </clip>
    </item>
    
</layer-list>

